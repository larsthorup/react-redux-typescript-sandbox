{
  "name": "typescript-sandbox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/history": "^4.7.4",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/ramda": "^0.26.40",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "coveralls": "^3.0.9",
    "history": "^4.10.1",
    "path-to-regexp": "^6.1.0",
    "prettier": "^1.19.1",
    "query-string": "^6.10.1",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.5",
    "user-event": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-comment": "echo https://github.com/facebook/create-react-app/issues/7491#issuecomment-520416528",
    "test": "tsc && react-scripts test --env=jsdom-fourteen",
    "test:ci": "tsc && react-scripts test --env=jsdom-fourteen --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "coveralls": "coveralls < coverage/lcov.info",
    "travis": "npm run test:ci && npm run coveralls"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 98
      }
    }
  }
}
